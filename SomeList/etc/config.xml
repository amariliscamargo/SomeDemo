<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
	<default>
		<payment>
			<invoice>
				<active>1</active>
				<model>SomeDemo\SomeList\Model\Payment\Invoice</model>
				<order_status>pending</order_status>
				<title>Invoice</title>
				<allowspecific>0</allowspecific>
				<group>offline</group>
			</invoice>
		</payment>
		<carriers>
			<freeshipping>
				<model>SomeDemo\SomeList\Model\Carrier\Freeshipping</model>
				<active>0</active>
				<title>Freeshipping</title>
				<name>Freeshipping</name>
				<price>0.00</price>
				<specificerrmsg>This shipping method is not available. To use this shipping method, please contact us.</specificerrmsg>
				<sallowspecific>0</sallowspecific>
			</freeshipping>
		</carriers>
		<inventory>
			<options>
				<out_of_stock_label/>
			</options>
		</inventory>
	</default>
</config>
