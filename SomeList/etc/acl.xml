<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="SomeDemo_SomeList::Blog" sortOrder="10" title="Blog">
					<resource id="SomeDemo_SomeList::Blog_save" sortOrder="10" title="Save Blog"/>
					<resource id="SomeDemo_SomeList::Blog_delete" sortOrder="20" title="Delete Blog"/>
					<resource id="SomeDemo_SomeList::Blog_update" sortOrder="30" title="Update Blog"/>
					<resource id="SomeDemo_SomeList::Blog_view" sortOrder="40" title="View Blog"/>
				</resource>
				<resource id="Magento_Backend::stores">
					<resource id="Magento_Backend::stores_settings">
						<resource id="Magento_Config::config">
							<resource id="SomeDemo_SomeList::config_somedemo_somelist" title="inventory"/>
						</resource>
					</resource>
				</resource>
			</resource>
		</resources>
	</acl>
</config>
